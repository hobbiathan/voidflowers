<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %>

<h1> sign in </h1>

<%= form_with url: login_path, local: :true do |form| %>

<%= form.label :username %>
<%= form.text_field :username %>
<%= form.label :password %>
<%= form.text_field :password %>

<%= form.submit "login" %>

<% end %>
